                                                                        owl:someValuesFrom
    pizza:HamTopping], [rdf:type owl:Restriction ;
                        owl:onProperty pizza:hasTopping ;
                        owl:someValuesFrom pizza:MozzarellaTopping], [rdf:type owl:Restriction ;
                                                                      owl:onProperty pizza:hasTopping ;
                                                                      owl:someValuesFrom
    pizza:OliveTopping], [rdf:type owl:Restriction ;
                          owl:onProperty pizza:hasTopping ;
                          owl:someValuesFrom pizza:TomatoTopping], [rdf:type owl:Restriction ;
                                                                    owl:onProperty pizza:hasTopping ;
                                                                    owl:allValuesFrom [rdf:type owl:Class ;
                                                                                       owl:unionOf
    (pizza:AnchoviesTopping
     pizza:ArtichokeTopping
     pizza:GarlicTopping
     pizza:HamTopping
     pizza:MozzarellaTopping
     pizza:OliveTopping
     pizza:TomatoTopping)]] ;
  rdfs:label "Siciliana"@en, "Siciliana"@pt ;
  skos:altLabel "Siciliana"@en, "Siciliana Pizza"@en ;
  skos:prefLabel "Siciliana"@en .

pizza:SlicedTomatoTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:TomatoTopping, [rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasSpiciness ;
                                        owl:someValuesFrom pizza:Mild] ;
  owl:disjointWith pizza:SundriedTomatoTopping ;
  rdfs:label "CoberturaDeTomateFatiado"@pt, "SlicedTomatoTopping"@en ;
  skos:prefLabel "Sliced Tomato"@en .

pizza:SloppyGiuseppe
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:NamedPizza, [rdf:type owl:Restriction ;
                                     owl:onProperty pizza:hasTopping ;
                                     owl:someValuesFrom pizza:GreenPepperTopping], [rdf:type owl:Restriction ;
                                                                                    owl:onProperty pizza:hasTopping ;
                                                                                    owl:someValuesFrom
    pizza:HotSpicedBeefTopping], [rdf:type owl:Restriction ;
                                  owl:onProperty pizza:hasTopping ;
                                  owl:someValuesFrom pizza:MozzarellaTopping], [rdf:type owl:Restriction ;
                                                                                owl:onProperty pizza:hasTopping ;
                                                                                owl:someValuesFrom
    pizza:OnionTopping], [rdf:type owl:Restriction ;
                          owl:onProperty pizza:hasTopping ;
                          owl:someValuesFrom pizza:TomatoTopping], [rdf:type owl:Restriction ;
                                                                    owl:onProperty pizza:hasTopping ;
                                                                    owl:allValuesFrom [rdf:type owl:Class ;
                                                                                       owl:unionOf
    (pizza:GreenPepperTopping
     pizza:HotSpicedBeefTopping
     pizza:MozzarellaTopping
     pizza:OnionTopping
     pizza:TomatoTopping)]] ;
  rdfs:label "SloppyGiuseppe"@en, "SloppyGiuseppe"@pt ;
  skos:altLabel "Sloppy Giuseppe"@en, "Sloppy Giuseppe Pizza"@en ;
  skos:prefLabel "Sloppy Giuseppe"@en .

pizza:Soho
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:NamedPizza, [rdf:type owl:Restriction ;
                                     owl:onProperty pizza:hasTopping ;
                                     owl:someValuesFrom pizza:GarlicTopping], [rdf:type owl:Restriction ;
                                                                               owl:onProperty pizza:hasTopping ;
                                                                               owl:someValuesFrom
    pizza:MozzarellaTopping], [rdf:type owl:Restriction ;
                               owl:onProperty pizza:hasTopping ;
                               owl:someValuesFrom pizza:OliveTopping], [rdf:type owl:Restriction ;
                                                                        owl:onProperty pizza:hasTopping ;
                                                                        owl:someValuesFrom
    pizza:ParmesanTopping], [rdf:type owl:Restriction ;
                             owl:onProperty pizza:hasTopping ;
                             owl:someValuesFrom pizza:RocketTopping], [rdf:type owl:Restriction ;
                                                                       owl:onProperty pizza:hasTopping ;
                                                                       owl:someValuesFrom
    pizza:TomatoTopping], [rdf:type owl:Restriction ;
                           owl:onProperty pizza:hasTopping ;
                           owl:allValuesFrom [rdf:type owl:Class ;
                                              owl:unionOf (pizza:GarlicTopping
                                                           pizza:MozzarellaTopping
                                                           pizza:OliveTopping
                                                           pizza:ParmesanTopping
                                                           pizza:RocketTopping
                                                           pizza:TomatoTopping)]] ;
  rdfs:label "Soho"@en, "Soho"@pt ;
  skos:altLabel "Soho"@en, "Soho Pizza"@en ;
  skos:prefLabel "Soho"@en .

pizza:Spiciness
  rdf:type owl:Class ;
  owl:equivalentClass [rdf:type owl:Class ;
                       owl:unionOf (pizza:Hot
                                    pizza:Medium
                                    pizza:Mild)] ;
  rdfs:subClassOf pizza:ValuePartition ;
  rdfs:comment
    "A ValuePartition that describes only values from Hot, Medium or Mild. NB Subclasses can themselves be divided up into further partitions."@en ;
  rdfs:label "Spiciness"@en, "Tempero"@pt ;
  skos:prefLabel "Spiciness"@en .

pizza:SpicyPizza
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:Pizza
                                           [rdf:type owl:Restriction ;
                                            owl:onProperty pizza:hasTopping ;
                                            owl:someValuesFrom pizza:SpicyTopping]) ;
                       rdf:type owl:Class] ;
  rdfs:label "PizzaTemperada"@pt, "SpicyPizza"@en ;
  skos:definition "Any pizza that has a spicy topping is a SpicyPizza"@en ;
  skos:prefLabel "Spicy Pizza"@en .

pizza:SpicyPizzaEquivalent
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:Pizza
                                           [rdf:type owl:Restriction ;
                                            owl:onProperty pizza:hasTopping ;
                                            owl:someValuesFrom [owl:intersectionOf (pizza:PizzaTopping
                                                                                    [rdf:type owl:Restriction ;
                                                                                     owl:onProperty pizza:hasSpiciness ;
                                                                                     owl:someValuesFrom pizza:Hot]) ;
                                                                rdf:type owl:Class]]) ;
                       rdf:type owl:Class] ;
  rdfs:comment
    "An alternative definition for the SpicyPizza which does away with needing a definition of SpicyTopping and uses a slightly more complicated restriction: Pizzas that have at least one topping that is both a PizzaTopping and has spiciness hot are members of this class."@en ;
  rdfs:label "PizzaTemperadaEquivalente"@pt, "SpicyPizzaEquivalent"@en ;
  skos:prefLabel "Spicy Pizza Equivalent"@en .

pizza:SpicyTopping
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:PizzaTopping
                                           [rdf:type owl:Restriction ;
                                            owl:onProperty pizza:hasSpiciness ;
                                            owl:someValuesFrom pizza:Hot]) ;
                       rdf:type owl:Class] ;
  rdfs:label "CoberturaTemperada"@pt, "SpicyTopping"@en ;
  skos:definition "Any pizza topping that has spiciness Hot"@en ;
  skos:prefLabel "Spicy"@en .

pizza:SpinachTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:VegetableTopping, [rdf:type owl:Restriction ;
                                           owl:onProperty pizza:hasSpiciness ;
                                           owl:someValuesFrom pizza:Mild] ;
  rdfs:label "CoberturaDeEspinafre"@pt, "SpinachTopping"@en ;
  skos:prefLabel "Spinach"@en .

pizza:SultanaTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:FruitTopping, [rdf:type owl:Restriction ;
                                       owl:onProperty pizza:hasSpiciness ;
                                       owl:someValuesFrom pizza:Medium] ;
  rdfs:label "CoberturaSultana"@pt, "SultanaTopping"@en ;
  skos:prefLabel "Sultana"@en .

pizza:SundriedTomatoTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:TomatoTopping, [rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasSpiciness ;
                                        owl:someValuesFrom pizza:Mild] ;
  rdfs:label "CoberturaDeTomateRessecadoAoSol"@pt, "SundriedTomatoTopping"@en ;
  skos:prefLabel "Sundried Tomato"@en .

pizza:SweetPepperTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:PepperTopping, [rdf:type owl:Restriction ;
                                        owl:onProperty pizza:hasSpiciness ;
                                        owl:someValuesFrom pizza:Mild] ;
  rdfs:label "CoberturaDePimentaoDoce"@pt, "SweetPepperTopping"@en ;
  skos:prefLabel "Sweet Pepper"@en .

pizza:ThinAndCrispyBase
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:PizzaBase ;
  rdfs:label "BaseFinaEQuebradica"@pt, "ThinAndCrispyBase"@en ;
  skos:prefLabel "Thin And Crispy Base"@en .

pizza:ThinAndCrispyPizza
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:Pizza
                                           [rdf:type owl:Restriction ;
                                            owl:onProperty pizza:hasBase ;
                                            owl:allValuesFrom pizza:ThinAndCrispyBase]) ;
                       rdf:type owl:Class] ;
  rdfs:label "ThinAndCrispyPizza"@en ;
  skos:prefLabel "Thin And Crispy Pizza"@en .

pizza:TobascoPepperSauce
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:SauceTopping, [rdf:type owl:Restriction ;
                                       owl:onProperty pizza:hasSpiciness ;
                                       owl:someValuesFrom pizza:Hot] ;
  rdfs:label "MolhoTobascoPepper"@pt, "TobascoPepperSauceTopping"@en ;
  skos:prefLabel "Tobasco Pepper Sauce"@en .

pizza:TomatoTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:VegetableTopping, [rdf:type owl:Restriction ;
                                           owl:onProperty pizza:hasSpiciness ;
                                           owl:someValuesFrom pizza:Mild] ;
  rdfs:label "CoberturaDeTomate"@pt, "TomatoTopping"@en ;
  skos:prefLabel "Tomato"@en .

pizza:UnclosedPizza
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:Pizza, [rdf:type owl:Restriction ;
                                owl:onProperty pizza:hasTopping ;
                                owl:someValuesFrom pizza:MozzarellaTopping] ;
  rdfs:comment
    "An unclosed Pizza cannot be inferred to be either a VegetarianPizza or a NonVegetarianPizza, because it might have other toppings."^^xsd:string ;
  rdfs:label "PizzaAberta"@pt, "UnclosedPizza"@en ;
  skos:prefLabel "Unclosed Pizza"@en .

pizza:ValuePartition
  rdf:type owl:Class ;
  rdfs:comment
    "A ValuePartition is a pattern that describes a restricted set of classes from which a property can be associated. The parent class is used in restrictions, and the covering axiom means that only members of the subclasses may be used as values. The possible subclasses cannot be extended without updating the ValuePartition class."^^xsd:string ;
  rdfs:label "ValorDaParticao"@pt, "ValuePartition"@en ;
  skos:prefLabel "Value Partition"@en .

pizza:VegetableTopping
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:PizzaTopping ;
  rdfs:label "CoberturaDeVegetais"@pt, "VegetableTopping"@en ;
  skos:prefLabel "Vegetable Topping"@en .

pizza:VegetarianPizza
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:Pizza
                                           [rdf:type owl:Class ;
                                            owl:complementOf [rdf:type owl:Restriction ;
                                                              owl:onProperty pizza:hasTopping ;
                                                              owl:someValuesFrom pizza:FishTopping]]
                                           [rdf:type owl:Class ;
                                            owl:complementOf [rdf:type owl:Restriction ;
                                                              owl:onProperty pizza:hasTopping ;
                                                              owl:someValuesFrom pizza:MeatTopping]]) ;
                       rdf:type owl:Class] ;
  rdfs:label "PizzaVegetariana"@pt, "VegetarianPizza"@en ;
  skos:definition
    "Any pizza that does not have fish topping and does not have meat topping is a VegetarianPizza. Note that instances of this class do not need to have any toppings at all."@en ;
  skos:prefLabel "Vegetarian Pizza"@en .

pizza:VegetarianPizzaEquivalent1
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:Pizza
                                           [rdf:type owl:Restriction ;
                                            owl:onProperty pizza:hasTopping ;
                                            owl:allValuesFrom pizza:VegetarianTopping]) ;
                       rdf:type owl:Class] ;
  rdfs:comment
    "Any pizza that only has vegetarian toppings or no toppings is a VegetarianPizzaEquiv1. Should be inferred to be equivalent to VegetarianPizzaEquiv2.  Not equivalent to VegetarianPizza because PizzaTopping is not covering"@en ;
  rdfs:label "PizzaVegetarianaEquivalente1"@pt, "VegetarianPizza1"@en ;
  skos:prefLabel "Vegetarian Pizza1"@en .

pizza:VegetarianPizzaEquivalent2
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:Pizza
                                           [rdf:type owl:Restriction ;
                                            owl:onProperty pizza:hasTopping ;
                                            owl:allValuesFrom [rdf:type owl:Class ;
                                                               owl:unionOf (pizza:CheeseTopping
                                                                            pizza:FruitTopping
                                                                            pizza:HerbSpiceTopping
                                                                            pizza:NutTopping
                                                                            pizza:SauceTopping
                                                                            pizza:VegetableTopping)]]) ;
                       rdf:type owl:Class] ;
  rdfs:comment
    "An alternative to VegetarianPizzaEquiv1 that does not require a definition of VegetarianTopping. Perhaps more difficult to maintain. Not equivalent to VegetarianPizza"@en ;
  rdfs:label "PizzaVegetarianaEquivalente2"@pt, "VegetarianPizza2"@en ;
  skos:prefLabel "Vegetarian Pizza2"@en .

pizza:VegetarianTopping
  rdf:type owl:Class ;
  owl:equivalentClass [owl:intersectionOf (pizza:PizzaTopping
                                           [rdf:type owl:Class ;
                                            owl:unionOf (pizza:CheeseTopping
                                                         pizza:FruitTopping
                                                         pizza:HerbSpiceTopping
                                                         pizza:NutTopping
                                                         pizza:SauceTopping
                                                         pizza:VegetableTopping)]) ;
                       rdf:type owl:Class] ;
  rdfs:comment
    "An example of a covering axiom. VegetarianTopping is equivalent to the union of all toppings in the given axiom. VegetarianToppings can only be Cheese or Vegetable or....etc."@en ;
  rdfs:label "CoberturaVegetariana"@pt, "VegetarianTopping"@en ;
  skos:prefLabel "Vegetarian Topping"@en .

pizza:Veneziana
  rdf:type owl:Class ;
  rdfs:subClassOf pizza:NamedPizza, [rdf:type owl:Restriction ;
                                     owl:onProperty pizza:hasTopping ;
                                     owl:someValuesFrom pizza:CaperTopping], [rdf:type owl:Restriction ;
                                                                              owl:onProperty pizza:hasTopping ;
                                                                              owl:someValuesFrom
    pizza:MozzarellaTopping], [rdf:type owl:Restriction ;
                               owl:onProperty pizza:hasTopping ;
                               owl:someValuesFrom pizza:OliveTopping], [rdf:type owl:Restriction ;
                                                                        owl:onProperty pizza:hasTopping ;
                                                                        owl:someValuesFrom
    pizza:OnionTopping], [rdf:type owl:Restriction ;
                          owl:onProperty pizza:hasTopping ;
                          owl:someValuesFrom pizza:PineKernels], [rdf:type owl:Restriction ;
                                                                  owl:onProperty pizza:hasTopping ;
                                                                  owl:someValuesFrom
    pizza:SultanaTopping], [rdf:type owl:Restriction ;
                            owl:onProperty pizza:hasTopping ;
                            owl:someValuesFrom pizza:TomatoTopping], [rdf:type owl:Restriction ;
                                                                      owl:onProperty pizza:hasTopping ;
                                                                      owl:allValuesFrom [rdf:type owl:Class ;
                                                                                         owl:unionOf (pizza:CaperTopping
                                                                                                      pizza:MozzarellaTopping
                                                                                                      pizza:OliveTopping
                                                                                                      pizza:OnionTopping
                                                                                                      pizza:PineKernels
                                                                                                      pizza:SultanaTopping
                                                                                                      pizza:TomatoTopping)]],
[rdf:type owl:Restriction ;
 owl:onProperty pizza:hasCountryOfOrigin ;
 owl:hasValue pizza:Italy] ;
  rdfs:label "Veneziana"@en, "Veneziana"@pt ;
  skos:altLabel "Veneziana"@en, "Veneziana Pizza"@en ;
  skos:prefLabel "Veneziana"@en .
#    Individuals
#################################################################

pizza:America
  rdf:type owl:NamedIndividual, pizza:Country, owl:Thing .

pizza:England
  rdf:type owl:NamedIndividual, pizza:Country, owl:Thing .

pizza:France
  rdf:type owl:NamedIndividual, pizza:Country, owl:Thing .

pizza:Germany
  rdf:type owl:NamedIndividual, pizza:Country, owl:Thing .

pizza:Italy
  rdf:type owl:NamedIndividual, pizza:Country, owl:Thing .
#    General axioms
#################################################################

[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:American
              pizza:AmericanHot
              pizza:Cajun
              pizza:Capricciosa
              pizza:Caprina
              pizza:Fiorentina
              pizza:FourSeasons
              pizza:FruttiDiMare
              pizza:Giardiniera
              pizza:LaReine
              pizza:Margherita
              pizza:Mushroom
              pizza:Napoletana
              pizza:Parmense
              pizza:PolloAdAstra
              pizza:PrinceCarlo
              pizza:QuattroFormaggi
              pizza:Rosa
              pizza:Siciliana
              pizza:SloppyGiuseppe
              pizza:Soho
              pizza:UnclosedPizza
              pizza:Veneziana)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:AnchoviesTopping
              pizza:MixedSeafoodTopping
              pizza:PrawnsTopping)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:ArtichokeTopping
              pizza:AsparagusTopping
              pizza:CaperTopping
              pizza:GarlicTopping
              pizza:LeekTopping
              pizza:MushroomTopping
              pizza:OliveTopping
              pizza:OnionTopping
              pizza:PepperTopping
              pizza:PetitPoisTopping
              pizza:RocketTopping
              pizza:SpinachTopping
              pizza:TomatoTopping)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:CheeseTopping
              pizza:FishTopping
              pizza:FruitTopping
              pizza:HerbSpiceTopping
              pizza:MeatTopping
              pizza:NutTopping
              pizza:SauceTopping
              pizza:VegetableTopping)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:ChickenTopping
              pizza:HamTopping
              pizza:HotSpicedBeefTopping
              pizza:PeperoniSausageTopping)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:FourCheesesTopping
              pizza:GoatsCheeseTopping
              pizza:GorgonzolaTopping
              pizza:MozzarellaTopping
              pizza:ParmesanTopping)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:GreenPepperTopping
              pizza:JalapenoPepperTopping
              pizza:PeperonataTopping
              pizza:SweetPepperTopping)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:Hot
              pizza:Medium
              pizza:Mild)] .


[rdf:type owl:AllDisjointClasses ;
 owl:members (pizza:IceCream
              pizza:Pizza
              pizza:PizzaBase
              pizza:PizzaTopping)] .


[rdf:type owl:AllDifferent ;
 owl:distinctMembers (pizza:America
                      pizza:England
                      pizza:France
                      pizza:Germany
                      pizza:Italy)] .
