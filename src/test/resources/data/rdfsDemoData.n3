[rdf:type owl:Ontology] .

eg:age
  rdf:type owl:AnnotationProperty .

eg:mum
  rdf:type owl:AnnotationProperty .

eg:Teenager
  rdf:type owl:Class .

eg:colin
  rdf:type owl:NamedIndividual, eg:Teenager ;
