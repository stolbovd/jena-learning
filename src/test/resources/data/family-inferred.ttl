@prefix : <http://example.com/owl/families/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

rdfs:DataType
  a owl:Class ;
  rdfs:subClassOf owl:Thing .

:Mary
  a owl:Thing, :Human, :Person, owl:NamedIndividual, :Woman ;
  :hasRelative :Mary ;
  :hasSpouse :John .

:hasAge
  a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:domain :Person ;
  rdfs:range xsd:integer ;
  rdfs:subPropertyOf owl:topDataProperty .

[owl:equivalentProperty :hasChild ;
 owl:inverseOf :hasParent] .

:hasUncle
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:propertyChainAxiom (:hasParent :hasBrother) .

:hasFather
  a owl:IrreflexiveProperty, owl:AsymmetricProperty, owl:ObjectProperty ;
  rdfs:subPropertyOf :hasParent ;
  rdfs:subPropertyOf [owl:equivalentProperty :hasParent ;
                      owl:inverseOf :hasChild] .

:Grandfather
  a owl:Class ;
  rdfs:subClassOf :Father ;
  rdfs:subClassOf [a owl:Class ;
                   owl:intersectionOf (:Man :Parent)] .

[a owl:AllDisjointClasses ;
 owl:members (:Woman :Man)] .

:Toddler
  a owl:Class ;
  rdfs:subClassOf :Person, :Human ;
  owl:equivalentClass [a owl:Restriction ;
                       owl:onProperty :hasAge ;
                       owl:someValuesFrom [a rdfs:Datatype ;
                                           owl:oneOf (1 2)]] .

:Mother
  a owl:Class ;
  rdfs:subClassOf :Woman, :Parent ;
  owl:equivalentClass [a owl:Class ;
                       owl:intersectionOf (:Woman :Parent)] .

:Man
  a owl:Class ;
  rdfs:subClassOf :Human, :Person .

:hasSpouse
  a owl:ObjectProperty, owl:SymmetricProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf :hasSpouse .

:John
  a :Parent, owl:Thing, :Human, :Father, owl:NamedIndividual, :Person, :Man, :HappyPerson ;
  :hasAge 151 ;
  :hasBrother :Petr ;
  :hasChild :Irina, :Susan ;
  :hasRelative :John ;
  :hasSpouse :Mary ;
  :hasWife :Mary .

:SocialRole
  a owl:Class ;
  rdfs:subClassOf owl:Thing .

:ChildlessPerson
  a owl:Class ;
  rdfs:subClassOf :Human, :Person ;
  owl:equivalentClass [a owl:Class ;
                       owl:intersectionOf (:Person
                                           [a owl:Class ;
                                            owl:complementOf :Parent])] .

:Person
  a owl:Class ;
  rdfs:comment "Represents the set of all people." ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass :Human .

:Woman
  a owl:Class ;
  rdfs:subClassOf :Human, :Person .

:Susan
  a :HappyPerson, owl:Thing, :Human, owl:NamedIndividual, :Person, :JohnsChildren, :Teenager ;
  :hasAge 15 ;
  :hasParent :John ;
  :hasRelative :Susan ;
  :hasUncle :Petr .

:Parent
  a owl:Thing, owl:NamedIndividual, owl:Class, :SocialRole ;
  rdfs:subClassOf :Human, :Person ;
  :hasRelative :Parent ;
  owl:equivalentClass [a owl:Restriction ;
                       owl:onProperty :hasChild ;
                       owl:someValuesFrom :Person] ;
  owl:equivalentClass [a owl:Class ;
                       owl:unionOf (:Mother :Father)] .

:Irina
  a :HappyPerson, :JohnsChildren, :Human, :Person, owl:NamedIndividual, :Toddler, owl:Thing ;
  :hasAge 2 ;
  :hasParent :John ;
  :hasRelative :Irina ;
  :hasUncle :Petr .

:hasGrandparent
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:propertyChainAxiom (:hasParent :hasParent) .

:parentOf
  a owl:ObjectProperty, owl:IrreflexiveProperty ;
  rdfs:subPropertyOf owl:topObjectProperty .

:hasBrother
  a owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty .

:JohnsChildren
  a owl:Class ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass [a owl:Restriction ;
                       owl:hasValue :John ;
                       owl:onProperty :hasParent] .

:hasChild
  a owl:IrreflexiveProperty, owl:ObjectProperty, owl:AsymmetricProperty ;
  rdfs:subPropertyOf owl:topObjectProperty .

:hasWife
  a owl:IrreflexiveProperty, owl:AsymmetricProperty, owl:ObjectProperty ;
  rdfs:domain :Man ;
  rdfs:range :Woman ;
  rdfs:subPropertyOf :hasSpouse ;
  rdfs:subPropertyOf [owl:equivalentProperty :hasSpouse ;
                      owl:inverseOf :hasSpouse] .

:Teenager
  a owl:Class ;
  rdfs:subClassOf :Person, :Human ;
  owl:equivalentClass [a owl:Restriction ;
                       owl:onProperty :hasAge ;
                       owl:someValuesFrom [a rdfs:Datatype, rdfs:DataType ;
                                           owl:onDatatype xsd:integer ;
                                           owl:withRestrictions ([xsd:minExclusive 12]
                                                                 [xsd:maxInclusive 19])]] .

:Father
  a owl:Class ;
  rdfs:subClassOf :Parent, :Man ;
  owl:equivalentClass [a owl:Class ;
                       owl:intersectionOf (:Man :Parent)] .

:Human
  a owl:Class ;
  rdfs:subClassOf owl:Thing .

:HappyPerson
  a owl:Class ;
  rdfs:subClassOf owl:Thing ;
  owl:equivalentClass [a owl:Restriction ;
                       owl:onProperty :hasChild ;
                       owl:someValuesFrom :HappyPerson] .

[a owl:Ontology] .

:hasRelative
  a owl:ObjectProperty, owl:ReflexiveProperty ;
  rdfs:subPropertyOf owl:topObjectProperty .

:hasParent
  a owl:AsymmetricProperty, owl:IrreflexiveProperty, owl:ObjectProperty ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf :hasChild ;
  owl:propertyDisjointWith :hasSpouse .

:Petr
  a owl:Thing, :Human, :Person, owl:NamedIndividual, :Man ;
  :hasRelative :Petr .
