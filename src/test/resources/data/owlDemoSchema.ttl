###  urn:x-hp:eg/Bundle
eg:Bundle rdf:type owl:Class .


###  urn:x-hp:eg/Computer
eg:Computer rdf:type owl:Class ;
            rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty eg:hasMotherBoard ;
                                                     owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] .


###  urn:x-hp:eg/GameBundle
eg:GameBundle rdf:type owl:Class ;
              rdfs:subClassOf eg:Bundle .


###  urn:x-hp:eg/GamingComputer
eg:GamingComputer rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( eg:Computer
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty eg:hasBundle ;
                                                               owl:someValuesFrom eg:GameBundle
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty eg:hasComponent ;
                                                               owl:hasValue eg:gamingGraphics
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


###  urn:x-hp:eg/GraphicsCard
eg:GraphicsCard rdf:type owl:Class .


